        "terminal.ansiMagenta": "#7F647A",
        "terminal.ansiCyan": "#66919B",
        "terminal.ansiWhite": "#D4D7DD",
        "terminal.ansiBrightBlack": "#475366",
        "terminal.ansiBrightRed": "#C66470",
