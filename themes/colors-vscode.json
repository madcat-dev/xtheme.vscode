        "terminal.ansiBrightCyan": "#86C3CC",
        "terminal.ansiBrightWhite": "#F4F8FF"
    },
    "tokenColors": [
        {
            "settings": {
                "background": "#15181E",
                "foreground": "#BEC1C6"
            }
        },
        {
            "name": "Comment",
            "scope": ["comment"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#5f8787"
            }
        },
        {
            "name": "Constant",
            "scope": ["constant"],
            "settings": {
                "foreground": "#66919B"
            }
        },
        {
            "name": "String",
            "scope": ["string"],
            "settings": {
                "foreground": "#778965"
            }
        },
        {
            "name": "Statement",
            "scope": [
                "entity",
                "keyword",
                "support",
                "storage",
                "meta",
                "support.class"
            ],
            "settings": {
                "foreground": "#D4D7DD"
            }
        },
        {
            "name": "Identifier",
            "scope": ["variable"],
            "settings": {
                "foreground": "#5D758C"
            }
        },
        {
            "name": "Type",
            "scope": [
                "storage.type",
                "support.type",
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#B79E6D"
            }
        },

        {
            "name": "Invalid",
            "scope": [
                "invalid"
            ],
            "settings": {
                "foreground": "2eeeeee",
                "background": "#89454D"
            }
        },

        {
            "scope": "token.info-token",
            "settings": {
                "foreground": "#6796e6"
            }
        },
        {
            "scope": "token.warn-token",
            "settings": {
                "foreground": "#cd9731"
            }
        },
        {
            "scope": "token.error-token",
            "settings": {
                "foreground": "#f44747"
            }
        },
        {
            "scope": "token.debug-token",
            "settings": {
                "foreground": "#b267e6"
            }
        },
        {
            "name": "this.self",
            "scope": "variable.language",
            "settings": {
                "foreground": "#FD971F"
            }
        }
    ]
}
